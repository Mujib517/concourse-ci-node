{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": false,
      "no-console": [
        true,
        "error"
      ],
      "quotemark": [
        true,
        "double"
      ],
      "variable-name": [
        "allow-snake-case",
        "allow-camel-case"
      ],
      "max-classes-per-file": false
    },
    "rulesDirectory": []
  }